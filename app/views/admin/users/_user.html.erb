<li>
  <p> <h2> <%= t ".user_name" %> <%= link_to user.name, admin_user_path(user) %> </h2></p>
  <p> <%= t ".id" %> <%= user.id %> </p>
  <p> <%= t ".email" %> <%= user.email %> </p>
  <p> <%= t ".role" %> <%= user.role %> </p>
  <p> <%= t ".joined_date" %> <%= user.created_at %> </p>
  <p>
    <%= t ".manage_user" %>
     <%= link_to t(".delete"), admin_user_path(user), method: :delete, 
       data: {confirm: t(".you_sure")} %>
     <%= link_to t(".edit"), edit_admin_user_path(user) %>
  </p>
</li>
